{
  "name": "seisplotjs-traveltime",
  "version": "1.1.0",
  "description": "Calculation of Earthquake wave travel times using the IRIS traveltime web service.",
  "main": "./lib/traveltime.js",
  "dependencies": {
    "rsvp": "^4.8.1"
  },
  "scripts": {
    "lint": "eslint 'src/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "babel --presets es2015 -d lib/ src/",
    "standalone": "browserify src/traveltime.js --standalone seisplotjs_traveltime -o seisplotjs_traveltime_${npm_package_version}_standalone.js -t [ babelify --presets [ es2015 ] ]",
    "prepare": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/crotwell/seisplotjs-traveltime.git"
  },
  "keywords": [
    "seismology",
    "traveltime",
    "event",
    "earthquake"
  ],
  "author": "Philip Crotwell <crotwell@seis.sc.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/crotwell/seisplotjs-traveltime/issues"
  },
  "homepage": "https://github.com/crotwell/seisplotjs-traveltime",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "eslint": "^4.16.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1"
  }
}
